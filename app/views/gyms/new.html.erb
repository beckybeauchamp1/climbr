<h2>Add New Climbing Center</h2>

<div class="form">
<%= form_for @gym do |f| %>

  <input name="authenticity_token" value="<%= form_authenticity_token %>" type="hidden">

  <p>
    <%= f.label :name %><br>
    <%= f.text_field :name %>
  </p>

  <p>
    <%= f.label :location %><br>
    <%= f.text_field :location %>
  </p>

  <p>
    <%= f.submit "ADD GYM" %>
  </p>

<% end %>

</div>
